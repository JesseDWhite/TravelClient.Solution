@{
  ViewData["Title"] = "Home Page";
}

<div class="text-center">
  <h1 class="display-4">Welcome</h1>

  <ol>
    @foreach (Destination destination in Model)
    {
      <ul>
        <li>Id: @destination.DestinationId</li>
        <li>Name: @destination.DestinationName</li>
        <li>Country: @destination.Country</li>
        <li>State or Province: @destination.StateProvince</li>
        <li>City: @destination.Category</li>
        <li>Rating: @destination.Rating</li>
        <li>Description: @destination.DestinationDesctription</li>
      </ul>
      <hr />
    }
  </ol>
</div>
